#include "util.h"
#include <Arduino.h>
char* append(char* first, char*second) {
	int len = strlen(second) + strlen(first);
	char* command = new char[len + 1];
	memset(command, 0, len + 1);
	memcpy(command, first, strlen(first));
	memcpy(command + strlen(first), second, strlen(second));
	return command;
}
