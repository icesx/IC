#include "SmartCarPark.h"
#include <Arduino.h>
#include "../sim900a/Sim900a.h"
//#include <SdCard.h>
Sim900a mySim900a;
SoftwareSerial mySerial(2,3);
//SdCard sdcard;
int onModulePin = 4;
void pownon() {
	pinMode(onModulePin, OUTPUT);
	digitalWrite(onModulePin, LOW);
	delay(1000);
	digitalWrite(onModulePin, HIGH);
	delay(2000);
	digitalWrite(onModulePin, LOW);
	delay(3000);
	Serial.println("pownon");
}
void setup() {
	Serial.begin(115200);
	Serial.println("Calling through GSM Modem");
	pownon();
	mySerial.begin(9600);
	delay(2000);
	mySim900a.prepare();
//	sdcard.testSDCard();
}

void loop() {
	delay(10000);
	mySim900a.transform();
}
